{% include 'incl/header.twig' %}

{% set imgs = ['a','b','c','d','e','f'] %}

<body>
    {% include 'incl/nav.twig' %}

    <div class="main">
        {{ content }}
    </div>
    <div class="main technology-grid" role="main">

        {% for i in imgs %}
            <a href="{{assets_url}}/img/gallery/{{i}}.jpg">
            <picture>
                <source media="(min-width: 800px)" srcset="image/gallery/{{i}}.jpg?w=90%&q=50">
                <source media="(min-width: 400px)" srcset="image/gallery/{{i}}.jpg?w=50%&q=50">
                <img src="image/gallery/{{i}}.jpg?w=25%&q=50" style="max-width: 100%;">
            </picture>
            </a>
        {% endfor %}

    </div>

    {% include 'incl/footer.twig' %}
</body>

</html>
